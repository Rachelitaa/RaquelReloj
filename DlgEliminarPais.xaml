<Window x:Class="Rellotge.DlgEliminarPais"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rellotge"
        mc:Ignorable="d"
        Title="DlgEliminarPais" Height="300" Width="300" Loaded="DlgEliminarPais_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="275*"/>
            <ColumnDefinition Width="17*"/>
        </Grid.ColumnDefinitions>
        <Label Content="Pais a eliminar:" HorizontalAlignment="Left" Margin="27,46,0,0" VerticalAlignment="Top" Width="94"/>
        <TextBox x:Name="TBNomPais" HorizontalAlignment="Left" Height="26" Margin="143,46,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="106" TextChanged="TBNomPais_TextChanged" PreviewKeyDown="TBNomPais_PreviewKeyDown" MaxLength="25" MaxLines="1"/>
        <Button x:Name="BtnOk" Content="Eliminar" HorizontalAlignment="Left" Margin="46,100,0,0" VerticalAlignment="Top" Width="75" Click="BtnOk_Click" IsDefault="True" IsEnabled="False"/>
        <Button x:Name="BtnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="156,100,0,0" VerticalAlignment="Top" Width="75" IsCancel="True"/>


    </Grid>
</Window>
